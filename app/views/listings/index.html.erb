<div class="center">
  
  <div class="row">
    <% @listings.each do |listing| %>
      <div class="col-md-3">
        <div class="thumbnail">
          <%= link_to image_tag(listing.image.url), listing%>
          <div class="caption">
            <h3><%= listing.name %></h3>
            <p><%= listing.duration%> <%= listing.location%></p>
            <% if (user_signed_in?) && (current_user == listing.user) %>
            <% end %>
          </div>
        </div>
      </div>
      <% end %>
  </div>
</div>

<br>

